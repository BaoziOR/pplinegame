@keyframes flowermove1 {
  0% {
    transform: rotate(20deg) translateY(-50rpx);
  }

  50% {
    transform: rotate(10deg) translateY(700rpx);
  }

  100% {
    transform: rotate(20deg) translateY(1500rpx);
  }
}

@keyframes flowermove2 {
  0% {
    transform: rotate(10deg) translateY(-20rpx);
  }

  50% {
    transform: rotate(5deg) translateY(600rpx);
  }

  100% {
    transform: rotate(10deg) translateY(1240rpx);
  }
}

@keyframes flowermove3 {
  0% {
    transform: rotate(-10deg) translateY(-60rpx);
  }

  50% {
    transform: rotate(-5deg) translateY(600rpx);
  }

  100% {
    transform: rotate(-10deg) translateY(1320rpx);
  }
}
