  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.starsAnim {
  position: absolute;
  width: 610rpx;
}

.stars1 {
  width: 38rpx;
  height: 39rpx;
  animation: starAnimation 4s both infinite;
  transform-origin: 50% 50%;
}

.stars2 {
  width: 33rpx;
  height: 34rpx;
  transform-origin: 50% 50%;
  animation: starAnimation 4s both infinite;
}

.stars3 {
  width: 33rpx;
  height: 34rpx;
  margin-bottom: -7rpx;
  margin-left: 7rpx;
  margin-right: 7rpx;
  transform-origin: 50% 50%;
  /* animation: starAnimation 4s both infinite; */
}

@keyframes starAnimation {
  0% {
    transform: scale(0.6, 0.6);
  }

  100% {
    transform: scale(1.1, 1.1);
  }
}

