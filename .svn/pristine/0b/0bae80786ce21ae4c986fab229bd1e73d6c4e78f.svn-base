.containerbg{
  width: 100%;
  height: 100%;
  background: rgba(1, 1, 1, 0.7);
  display: flex;
  justify-content: center;
  align-items: center;
  align-self: center;
  position: fixed;
  z-index: 100;
}

.middle{
  width: 614rpx;
  height: 880rpx;
  background-color: #fdfae9;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
}
.title{
  width: 100%;
  height:90rpx;
  background-color:#82cec4;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  color: white;
  font-size:  41rpx;
  font-weight: bolder;
  text-shadow: 0 5rpx #4ba698;
  display: flex;
  justify-content: center;
  align-items: center;
  align-self: center;
}

.skinClose{
  position: absolute;
  width: 51rpx;
  height:49rpx;
  right: 90rpx;
  border-radius: 10rpx;
  color: white;
  font-size: 50rpx;
  font-weight: bolder;
  background-color: #59b2a6;
  display: flex;
  align-items: center;
  justify-content: center;
}
.skinClose image{
  width: 34rpx;
  height:34rpx;
}
.skines{
  width: 534rpx;
  height: 730rpx;
  margin-top: 20rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.skingroup{
  width:534rpx;
  height:218rpx;
  /* background-color: red; */
  margin-left: 10rpx;
  margin-right: 10rpx;
  display: flex;
}
.skinitem{
  width:140rpx;
  height:228rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-right: 47rpx;
}

.bigCircle{
  width:146rpx;
  height:146rpx;
  border-radius: 73rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.innerCircle{
  width:134rpx;
  height:134rpx;
  border-radius: 67rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.skin{
  width: 76rpx;
  height: 76rpx;
  border-radius: 38rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.skin image{
  width:76rpx;
  height: 76rpx;
}
.useNow{
  width:56rpx;
  height:58rpx;
  margin-left: 96rpx;
  margin-top:-54rpx;
}
.useNow image{
  width:56rpx;
  height:58rpx;
}
.clickUse{
  width: 100%;
  height:45rpx;
  border-radius: 8rpx;
  margin-top: -16rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}

.collectFrag{
  width: 100%;
  height:25rpx;
  border-radius: 8rpx;
  background-color: #bdad9e;  
  display: flex;
  flex-direction: column;
  justify-content: center;
  /* align-items: center; */
}
.fragimage{
  width: 43rpx;
  height: 52rpx;
  position: absolute;  
  margin-left:-10rpx; 
}
 .fragCnt{
  margin-top: -28rpx;
  margin-left: 42rpx;
  font-size: 25rpx;
