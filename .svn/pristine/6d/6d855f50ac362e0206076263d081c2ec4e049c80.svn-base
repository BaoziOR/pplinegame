  border-radius: 10rpx;
  z-index: -1;
}

@keyframes circle-action {
  0% {
    transform: translateY(0rpx) rotate(45deg);
  }
  100% {
    transform: translateY(1400rpx) rotate(360deg);
  }
}

@keyframes circle-action1 {
  0% {
    animation-delay: 20s;
    transform: translateY(100rpx) rotate(90deg);
  }
  100% {
    transform: translateY(1400rpx) rotate(360deg);
  }
}

