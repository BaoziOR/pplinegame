@keyframes boxShake{
  0%{
    transform: rotate(-10deg) scale(1,1);
  }
  25%{
    transform: rotate(0deg) scale(0.98,0.98);
  }
  50%{
    transform: rotate(10deg) scale(1,1);
  }
  75%{
    transform: rotate(0deg) scale(0.98,0.98);
  }
  100%{
    transform: rotate(-10deg) scale(1,1);
  }
}

