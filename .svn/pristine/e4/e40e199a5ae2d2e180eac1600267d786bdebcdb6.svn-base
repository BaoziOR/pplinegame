.award{
  width: 610rpx;
  height: 610rpx;
  background-color: #fefae8;
  border-bottom-left-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
  box-shadow: -2rpx 10rpx #48453a;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.awardTxt{
  width:560rpx;
  height:260rpx;
  border-radius: 10rpx;
  background-color:#f6edd8;
  font-size: 28rpx;
  font-weight: bold;
  color: #ab9379;
  display: flex;
  flex-direction: column;
}
.awardItems{
  width:560rpx;
  height:260rpx;
  margin-top:30rpx;
  display: flex;
  flex-direction:column;
  align-items:center;
  font-size:31rpx;
  font-weight:bolder;
  color:#82562b;
}
.awardList{
  width: 560rpx;
  height: 210rpx;
  display: flex;
  justify-content: space-around;
}
.itemABg{
  width:175rpx;
  height:175rpx;
  background-color:#f9f4e1;
  border-radius: 90rpx;
  margin-top: 22rpx;
  display: flex;
  justify-content: center;
}
.itemA{
  width:160rpx;
  height:160rpx;
  background-color:#f2e9d0;
  border-radius: 80rpx;
  align-self: center;
  display: flex;
  flex-direction:  column;
  justify-content: center;
  align-items: center;
}
.itemIcon{
  width: 61rpx;
  height: 75rpx;
}
.awardGet{
  width:334rpx;
  height:124rpx;
  margin-top:-30rpx; margin-bottom:150rpx;display: flex;align-items:center;justify-content:center;color:white;font-size:40rpx;font-weight:bolder; background-color:#a2cf4f;box-shadow:0 10rpx #79a235;border-radius:15rpx;
}
