.containers{
    position: fixed;
  width: 750rpx;
  height: 1334rpx;
  background-color: #fefae8;
  display: flex;
  flex-direction: column;
  align-items: center;
}

/* 列表 */
.listup{
  width: 638rpx;
  height: 18rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  margin-top:97rpx; 
  background-color: #f0d2a7;
}

.list{
  width: 638rpx;
  height: 790rpx;
  margin-top:4rpx; 
  background-color: #f6e1c0;
  display: flex;
  flex-direction: column;
  overflow: hidden;    
}

.listItem{
  width: 638rpx;
  height:87rpx; 
  display: flex;
  margin-bottom: 10rpx;
  color: #8a5420;
  font-size: 30rpx;
}
.listitemBg{
  background-color: #e78cc9;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
rankIconbg{
  width: 62rpx;
  height: 55rpx;  
  margin-left: 25rpx;
  display: flex;
  align-self: center;
  align-items: center;
  justify-content: center;
  margin-right: 16rpx;
  background-color: red;
}
.rankIcon{
  width: 62rpx;
  height: 55rpx;
  margin-top: 15rpx;
  margin-left: 25rpx;
  margin-right: 18rpx;
   display: flex;
  align-self: center;
  align-items: center;
  justify-content: center; 
  font-size: 30rpx;
}

.rank1{
  color: #cc830c;  
}
.rank2{
  color: #6f6478;  
}
.rank3{
  color: #8a5420;  
}
.itemRank{
  width: 53rpx;
  height: 53rpx;
  margin-left: 25rpx;
  border-radius: 26rpx;
  display: flex;
  align-self: center;
  align-items: center;
  justify-content: center;
  background-color: #ecc5a1;
  margin-right: 22rpx;
}

.rankSelect{  
  background-color: #cf67ad;
  color: white;
  font-size: 31rpx;
}

.icon{
  width: 71rpx;
  height: 71rpx;
  align-self: center;
  margin-right: 30rpx;
  border-radius: 10rpx;
}

.nickname{
  width: 250rpx;
  color: #8a5420;
  align-self: center;
}

.score{
  font-size: 30rpx;
  color: #8a5420;
  align-self: center;
  margin-left: 50rpx;
}
.down
{
  margin-top: 114rpx;
  font-size: 34rpx;
  color:#d39066;
}

